<% provide(:title, "Log In") %> 
<% content_for :main do%>
	<% breadcrumb :log_in %>
	<h4>Log In</h4>
	<%= form_for(:user_session, url: login_path) do |f| %>
		<p>
			<%= f.label :username %><br />
			<%= f.text_field :username %>
		</p>
		<p>
			<%= f.label :password %><br />
			<%= f.password_field :password %>
		</p>
		<p>
			<%= f.submit "Login", class: "button" %><br />
		</p>
		<br /><%= link_to "Forgot your password?", new_password_reset_path %>
	<%end%>
<%end%>
