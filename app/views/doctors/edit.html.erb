<% provide(:title, "Edit Doctor") %> 
<% content_for :main do%>
	<h4>Edit Doctor</h4>
	<%= render "form" %>
	<p>
		<% unless !senior_staff_member_is_logged_in %>
			<%= link_to "<< Back to Doctors", doctors_path %> | 
		<%end%>
		<% unless !admin_is_logged_in %>
			<%= link_to "Destroy", @doctor, method: :delete, data: { confirm: "Are you sure?" } %> | 
		<%end%>
		<%= link_to (@doctor.dr_first_name_last_name), doctor_path(@doctor) %>
	</p>

<%end%>
